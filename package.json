{
  "name": "@ticatec/node-common-library",
  "version": "2.1.10",
  "description": "A comprehensive Node.js database access framework providing robust abstractions for database connection management, SQL execution, transaction handling, pagination, and dynamic query building.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    "README_CN.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "prebuild": "npm run lint",
    "prepublishOnly": "npm run build",
    "publish-public": "npm publish --access public",
    "lint": "echo \"Linting completed\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "nodejs",
